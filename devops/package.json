    "typescript": "~5.6.3",
    "rmdir-cli": "^2.0.6"
  },
  "dependencies": {
    "aws-cdk-lib": "2.173.4",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
